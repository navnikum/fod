{
  "compileOnSave": true,
  "compilerOptions": {
    "allowJs": false,
    "alwaysStrict": false,
    "baseUrl": ".",
    "checkJs": false,
    "lib": [
      "dom",
      "es5",
      "scripthost",
      "es2015.core",
      "es2015.collection",
      "es2015",
      "esnext.asynciterable"
    ],
    "module": "amd",
    "moduleResolution": "node",
    "suppressImplicitAnyIndexErrors": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "removeComments": false,
    "noEmitOnError": false,
    "noImplicitAny": false,
    "noImplicitThis": false,
    "outDir": "tmp",
    "paths": {
      "knockout": ["./node_modules/knockout"],
      "ojs/*": ["./node_modules/@oracle/oraclejet/dist/types/*"],
      "*": ["./src/ts/jet-composites/*", "./node_modules/*"]
    },
    "sourceMap": true,
    "strictFunctionTypes": false,
    "strictNullChecks": false,
    "target": "es6",
    "types": [
      "./node_modules/@types/chai",
      "./node_modules/@types/expect",
      "./node_modules/@types/karma-fixture",
      "./node_modules/@types/mocha",
      "./node_modules/@types/selenium-webdriver",
      "./node_modules/@types/sinon",
      "./node_modules/@types/uuid"
    ],
    "typeRoots": [
      "./node_modules/@types",
      "./node_modules/@oracle/oraclejet/dist/types"
    ]
  },
  "include": [
    "./src/ts/**/*",
    "./tests/unit/**/*.ts",
    "./node_modules/@oracle/oraclejet/dist/types/requirejs-plugins.d.ts",
    "./node_modules/karma-viewport/dist/index.d.ts"
  ]
}
