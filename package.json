    "@spectra/oj-spectra-webdriver": "https://artifacthub-phx.oci.oraclecorp.com/atgpf-npm-local/@spectra/oj-spectra-webdriver/-/@spectra/oj-spectra-webdriver-2210.0.0-alpha.2.2.tgz",
    "@spectra/automation-utils": "0.0.10",
