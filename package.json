    "@oracle/grunt-vb-build": "https://static.oracle.com/cdn/vb/tools/npm/grunt-vb-build/grunt-vb-build-2210.0.1.tar.gz",
    "@oracle/oraclejet-webdriver": "https://artifacthub-phx.oci.oraclecorp.com/ojet-dev-local/oracle-oraclejet-webdriver-12.1.1.tgz",
