  "scripts": {
    "test:gat:ci": "ts-mocha -p ./tests/integ/tsconfig.json  --testEnv=gat --gridURL=http://gathub.us.oracle.com/wd/hub --opts  ./tests/integ/mocha.opts",
    "audit": "./node_modules/.bin/grunt vb-audit --audit.files='services/**','webApps/**','dynamicLayouts/**','!**/node_modules/**' --url:ce=http://exchange.oraclecorp.com/api/0.2.0 --audit.outputformat='jet' --audit.outputfile='auditoutput.json' --force",
    "build": "grunt vb-clean vb-process-local --url:ce=http://exchange.oraclecorp.com/api/0.2.0 vb-package --mode=fa",
    "bundle:cdn": "grunt fa-cdn --url:ce=http://exchange.oraclecorp.com/api/0.2.0",
    "bundle:clean": "grunt vb-clean",
    "lint": "./node_modules/.bin/eslint --ext .js --ext .json --ext .htm? ./services/ ./webApps/",
    "lint:fix": "npm run lint:js:html -- --fix",
    "lint:js:html": "eslint --ext .js --ext .htm?  --ext .json .",
    "start": "grunt serve",
    "start:telemetry:mock": "node node_modules/telemetry-mock-server/index.js",
    "test:unit:ci": "node -e 'console.log(\"No Unit Tests yet\")'",
    "test:integ:mocha": "ts-mocha -p ./tests/integ/tsconfig.json --opts ./tests/integ/mocha.opts --baseURL=https://localhost:4000/vp/ordersref --hoverfly",
    "test:integ:mocha:mock": "npm run test:integ:mocha -- --grep=@dit -i",
    "test:integ:mocha:capture": "ts-mocha -p ./tests/integ/tsconfig.json --opts ./tests/integ/mocha.opts --baseURL=https://localhost:4000/vp/ordersref --hoverfly --capture-stateful",
    "test:integ:mocha:mock:capture": "npm run test:integ:mocha:capture -- --grep=@dit -i",
    "test:integ:ci:disabled": "START_SERVER_AND_TEST_INSECURE=1 start-server-and-test start https-get://localhost:4000/vp/ordersref start:telemetry:mock https://localhost:3000/trace test:integ:mocha:mock",
    "test:integ:ci": "npm run test:integ",
    "test:integ": "ts-mocha -p ./tests/integ/tsconfig.json --opts ./tests/integ/mocha.opts",
    "vb:audit": "grunt vb-audit --audit.files='services/**','webApps/**','dynamicLayouts/**','!/node_modules/*' --url:ce=http://exchange.oraclecorp.com/api/0.2.0 --audit.outputformat='jet' --audit.outputfile='auditoutput.json' --force",
    "update:versions": "node build-config/updateVersions.js",
    "vb:actionChain": "grunt vb-clean vb-process-local --url:ce=http://exchange.oraclecorp.com/api/0.2.0 vb-test",
    "test:upstream": "npm run test:integ:mocha -- --grep=@upstream",
    "test:ux": "npm run test:integ:mocha -- --'grep=@ux|@upstream'",
    "copyright": "grunt copyright",
    "reset:trainings": "node ./build-config/resetTrainings.js"
  },
  "com_oracle_visual_builder": {
    "upgrade": "none"
  },
  "dependencies": {
    "moment-timezone": "^0.5.34"
  }
}
